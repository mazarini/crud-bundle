{% extends '@MazariniCrud/pagination/_list.html.twig' %}

{% block title %}
	emptyRow index
{% endblock %}

{% block page_title %}
	<th>Id</th>
	<th>Col3</th>
	<th>Col4</th>
	<th>actions</th>
{% endblock page_title %}

{% block page_row %}
	<td>{{ entity.id }}</td>
	<td>{{ entity.col3 }}</td>
	<td>{{ entity.col4 }}</td>
{% endblock page_row %}

{% block page_not_found %}
	{% include '@MazariniCrud/pagination/nothing.html.twig'with {'span': 4} %}
{% endblock page_not_found %}

{% block page_new %}
	{% include '@MazariniCrud/pagination/new.html.twig' with {'span': 3} %}
{% endblock page_new %}
