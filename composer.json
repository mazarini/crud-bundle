{
    "name": "mazarini/crud-bundle",
    "description": "Simple CRUD for symfony",
    "type": "symfony-bundle",
    "keywords": [
        "symfony",
        "CRUD",
        "pagination"
    ],
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "mazarini/bootstrap-bundle": "^1.0@dev",
        "mazarini/pagination-bundle": "^1.0@dev",
        "mazarini/tools-bundle": "^1.0@dev",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/dotenv": "^4.4|^5.0",
        "symfony/flex": "^1.5",
        "symfony/form": "^4.4|^5.0",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.4|^5.0",
        "symfony/security-csrf": "^4.4|^5.0",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "^4.4|^5.0",
        "symfony/yaml": "^4.4|^5.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.2",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "mazarini/package": "^1.1",
        "mazarini/test-bundle": "^1.0@dev",
        "symfony/debug-pack": "^1.0",
        "symfony/maker-bundle": "^1.14",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mazarini\\CrudBundle\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "src/",
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.0.*"
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
